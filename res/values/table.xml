<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <string name="replacer">Пример текста</string>
    <string name="Mail.App.Name">Пони-почтовик</string>

    <drawable name="Mail.Bar.BG">@drawable/bar_bg_tiled</drawable>
    <color name="Mail.Bar.Font.Color">#dedede</color>
    <dimen name="Mail.Bar.Height">54dp</dimen>
    <dimen name="Mail.Bar.Font.Size">16sp</dimen>

    <color name="Mail.Label.BG">#333333</color>
    <color name="Mail.Label.Selected.BG">#333355</color>
    <color name="Mail.Label.Primary.Color">#dedede</color>
    <dimen name="Mail.Label.Primary.Size">16sp</dimen>
    <color name="Mail.Label.Secondary.Color">#adadad</color>
    <dimen name="Mail.Label.Secondary.Size">12sp</dimen>
    <dimen name="Mail.Label.Count.Size">12sp</dimen>
    <color name="Mail.Label.Count.Primary">#dedede</color>
    <color name="Mail.Label.Count.Green">#3fa744</color>
    <integer name="Mail.Label.Cut">2</integer>
    <string name="Mail.Label.AndOther">, и ещё %d пони</string>

    <string name="Mail.Table.Label">Список писем</string>
    <string name="Mail.View.Label">Просмотр письма</string>

    <plurals name="Mail_Label_Comments">
        <item quantity="one">комментарий</item>
        <item quantity="few">комментария</item>
        <item quantity="many">комментариев</item>
        <item quantity="two">комментария</item>
        <item quantity="other">комментариев</item>
        <item quantity="zero">комментариев</item>
    </plurals>

    <plurals name="Mail_Label_NewComments">
        <item quantity="one">новый</item>
        <item quantity="few">новых</item>
        <item quantity="many">новых</item>
        <item quantity="two">новых</item>
        <item quantity="other">новых</item>
        <item quantity="zero">новых</item>
    </plurals>

    <plurals name="Mail_Actions_Delete">
        <item quantity="one">Удалено $n письмо.</item>
        <item quantity="few">Удалено $n письма.</item>
        <item quantity="many">Удалено $n писем.</item>
        <item quantity="two">Удалено $n письма.</item>
        <item quantity="other">Удалено $n писем.</item>
        <item quantity="zero">Удалено $n писем.</item>
    </plurals>

    <plurals name="Mail_Actions_Read">
        <item quantity="one">$n письмо отмечено как прочитанное.</item>
        <item quantity="few">$n письма отмечены как прочитанные.</item>
        <item quantity="many">$n писем отмечено как прочитанные.</item>
        <item quantity="two">$n письма отмечено как прочитанные.</item>
        <item quantity="other">$n писем отмечены как прочитанные.</item>
        <item quantity="zero">$n писем отмечено как прочитанные.</item>
    </plurals>

    <!-- Ибо задолбало проставлять везде это вручную. -->
    <style name="Luna" parent="@android:style/Theme.Holo.Light.NoActionBar">
        <item name="android:textCursorDrawable">@null</item>
    </style>

    <string name="Mail.Actions.Error">Ошибка при выполнении операции над письмами.</string>

    <string name="Mail.Notification.Comment">Новый ответ в письме</string>
    <string name="Mail.Notification.Letter">Вам пришло письмо</string>

</resources>